{
  "name": "1brickatime-site",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:ping": "node -r dotenv/config scripts/db-ping.mjs dotenv_config_path=.env.local",
    "seed:minifigs": "node -r dotenv/config scripts/seed-minifigs.mjs dotenv_config_path=.env.local",
    "vercel:preview": "vercel --yes",
    "vercel:prod": "vercel --prod",
    "api:curl:preview": "curl -s \"$PREV/api/minifigs-live?limit=2\" | jq",
    "api:curl:prod": "curl -s \"$PROD/api/minifigs-live?limit=2\" | jq"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "mongodb": "^6.8.0",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "typescript": "^5.5.4"
  }
}
